{
  "buildCommand": "cd ../.. && npm run build:atta",
  "outputDirectory": "../../apps/atta/dist",
  "installCommand": "cd ../.. && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://api.attadia.com"
  }
}